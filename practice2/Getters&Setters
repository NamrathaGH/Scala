class Student{
    var name:String=""
    private var age:Int=0
    def set_age(x:Int)={
        age = x
    }
    def get_age():Int={
        return age
    }
}
object MyClass{
    def main(args: Array[String]) {
        var s1= new Student()
        s1.name ="Namratha"
        s1.set_age(20)
        var res = s1.get_age()
        println("Name: "+s1.name)
        println("Age: "+res)
    }
}
